  <script>
    $(document).on('click', '.size-chart-tab a', function(){
      var data = $(this).attr('data');
      $('.size-chart-tab a').removeClass('active');
      $(this).addClass('active');       
      $('.size-table').removeClass('active');
      $('.size-table').each(function(){
      var id = $(this).attr('id');
        if(data == id){
          $(this).addClass('active');
        }
      });
    });
    $(document).on('click', '.close-size-chart', function(){
      $('body').removeClass('size-chart-body');
    });
    $(document).on('click', '.size-chart-text', function(){
      $('body').addClass('size-chart-body');
    });
    
  </script>
