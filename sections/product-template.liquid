<script>
  $(document).ready(function () {
    $(document).on("scroll", onScroll);
    $('.product-desktop-dots a[href^="#"]').on('click', function (e) {
      e.preventDefault();
      $(document).off("scroll");

      $('.product-desktop-dots li').each(function () {
        $(this).removeClass('active');
      })
      $(this).addClass('active');

      var target = this.hash,
          menu = target;
      $target = $(target);
      $('html, body').stop().animate({
        'scrollTop': $target.offset().top+2
      }, 500, 'swing', function () {
        window.location.hash = target;
        $(document).on("scroll", onScroll);
      });
    });
  });
  
  $(document.body).on('touchmove', onScroll1); // for mobile
  $(window).on('scroll', onScroll1); 
  
  function onScroll1(event){
    var scrollPos = $(document).scrollTop();
    $('.product-desktop-dots li a').each(function () {
        var currLink = $(this);
        var refElement = $(currLink.attr("href"));
        if (refElement.position().top <= scrollPos && refElement.position().top + refElement.height() > scrollPos) {
            $('.product-desktop-dots li a').removeClass("active");
            currLink.addClass("active");
        }
        else{
            currLink.removeClass("active");
        }
    });
}
  
  $(document.body).on('touchmove', onScroll); // for mobile
  $(window).on('scroll', onScroll); 

  // callback
  function onScroll(){ 
    var window_h = window.innerHeight + $(window).scrollTop();
    var prod_img_h = $('#product_photo_thumbs').innerHeight() + 47;
    if($('body').hasClass('swipe-desc-top')){
    if(window_h >= prod_img_h){
      $('.template-product .product-shop.mobile-prod-detail').css({'top':'42px'});
      $('body').addClass('swipe-desc-top');
      $('body').removeClass('scrroll');
    }
    }else{
      if(window_h >= prod_img_h){
      /* indigo mobile bottom popup */
      $('.template-product .product-shop.mobile-prod-detail').css({'top':'42px', 'height':'100%', 'position':'fixed', 'bottom':'auto', 'overflow':'visible'});
        /* indigo mobile bottom popup */
      $('body').addClass('swipe-desc-top');
      $('body').removeClass('scrroll');
    }else{
      /* indigo mobile bottom popup */
      $('.template-product .product-shop.mobile-prod-detail').css({'height':'150px', 'position':'sticky', 'bottom':'-30px', 'overflow':'visible'});
      /* indigo mobile bottom popup */
        $('body').removeClass('swipe-desc-top');
      $('body').addClass('scrroll');
    }
    }
  }
  
  $(document).ready(function(){
    $(function() {
      $(document).on('click', '.product-desc-close-wc', function(){
        $(".mobile-full-height").animate({scrollTop: 0}, 100);
        /* indigo mobile bottom popup */
        $('.template-product .product-shop.mobile-prod-detail').css({'top':'0', 'height':'150px', 'position':'sticky', 'bottom':'-30px', 'overflow':'visible'});
        /* indigo mobile bottom popup */
        $('body').removeClass('swipe-desc-top');
        $(window).scrollTop(0);
      });   
      $(".template-product .product-shop.mobile-prod-detail").swipe({
        swipeStatus:function(event, phase, direction, distance, duration, fingers)
        {
          if (phase=="move" && direction =="up") {
            $('.template-product .product-shop.mobile-prod-detail').css({'top':'42px','height':'100%', 'position':'fixed', 'bottom':'auto', 'overflow':'visible'});
            $('body').addClass('swipe-desc-top');     
            $('body').removeClass('scrroll');
            return false;          
          }
          if($('.mobile-full-height').hasClass('wc_inactive')){
            if (direction =="down") {
              $('.template-product .product-shop.mobile-prod-detail').css({'top':'calc(100% - 132px - 0px - 0px + 7px)'});
              $('body').removeClass('swipe-desc-top');        
              $(window).scrollTop(0);
              return false;
            }
          }  
          $('.mobile-full-height').scroll(function(){
            if($(this).scrollTop() == 0){
              $('.mobile-full-height').addClass('wc_inactive');

            }else{
              $('.mobile-full-height').removeClass('wc_inactive');
            }         
          });
        }
      });     
    }); 
    $(document).on('click', '.top-line-wc', function(){
      $('.template-product .product-shop.mobile-prod-detail').css({'top':'42px','height':'100%', 'position':'fixed', 'bottom':'auto', 'overflow':'visible'});
      $(window).scrollTop(0);
            $('body').addClass('swipe-desc-top');     
            $('body').removeClass('scrroll');
    });
    $(document).on('click', '.selectize-dropdown-content .option .variant-title-wc', function(){
      var opt_t = $(this).text();
      console.log(opt_t);
      $('.selectize-input .item .variant-title-wc').text(opt_t);
    });
    
  });
</script>
