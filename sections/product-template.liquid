<script>
  $(document).on('click', '.wc_color_swatch', function(){
    $(this).toggleClass('wc_color_swatch_open');
  });
  $('body').click(function(e){
    var container = $(".wc_color_swatch");
    // if the target of the click isn't the container nor a descendant of the container
    if (!container.is(e.target) && container.has(e.target).length === 0) 
    {
      $('.wc_color_swatch').removeClass('wc_color_swatch_open');
    }
  });
</script>

